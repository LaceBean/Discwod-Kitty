/* Change the mute button */
button[aria-label="Mute"]:not([aria-checked="true"]) {
    background: url('https://imgur.com/c2QOA9G.png') no-repeat center center !important; /* just replace the URL to change the image */
    background-size: contain !important;
}

/* Change the unmute button */
button[aria-label="Mute"][aria-checked="true"] {
    background: url('https://imgur.com/WPx8fZM.png') no-repeat center center !important; /* just replace the URL to change the image */
    background-size: contain !important;
}

/* Hide the mute icon */
button[aria-label="Mute"] svg,
button[aria-label="Mute"] path {
    display: none !important;
    opacity: 0 !important;
    width: 0px !important;
    height: 0px !important;
}

